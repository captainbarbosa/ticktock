<h2>Sign Up</h2>

<%= form_for @developer, url: signup_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>


  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>


  <%= f.submit "Sign up" %>
<% end %>
